<dotnetnuke type="Package" version="5.0">
  <packages>
    <package name="MHG.Portals._default.Skins.LandingSkin" type="Skin" version="00.00.01">
      <friendlyName>LandingSkin</friendlyName>
      <description>A responsive DNN skin using Bootstrap 3. Requires installation via the Host/Extensions page.</description>
      <owner>
        <name>MHGInsurance.com</name>
        <organization>MHGInsurance.com</organization>
        <url>http://www.MHGInsurance/</url>
        <email>modules@MHGInsurance.com</email>
      </owner>
      <azureCompatible>true</azureCompatible>
      <license src="license.txt"></license>
      <releaseNotes src="releasenotes.txt"></releaseNotes>
      <dependencies>
        <dependency type="CoreVersion">07.01.00</dependency>
      </dependencies>
      <components>
        <component type="ResourceFile">
          <resourceFiles>
            <basePath>portals/_default/skins/LandingSkin</basePath>
            <resourceFile>
              <name>Resources.zip</name>
            </resourceFile>
          </resourceFiles>
        </component>
        <component type="Assembly">
          <assemblies>
            <assembly>
              <name>LandingSkin.dll</name>
              <path>bin</path>
            </assembly>
          </assemblies>
        </component>
        <component type="Skin">
          <skinFiles>
            <skinName>LandingSkin</skinName>
            <basePath />
            <skinFile>
              <name>Home.ascx</name>
            </skinFile>
            <skinFile>
              <name>Edit.ascx</name>
            </skinFile>
            <skinFile>
              <name>PopupSkin.ascx</name>
            </skinFile>
          </skinFiles>
        </component>
      </components>
    </package>
    <package name="MHGWeb.vs2015.Portals._default.Skins.Containers.LandingSkin" type="Container" version="00.00.01">
      <friendlyName>LandingSkin</friendlyName>
      <description>Containers for the LandingSkin Skin package.</description>
      <owner>
        <name>MHGInsurance.com</name>
        <organization>MHGInsurance.com</organization>
        <url>http://www.MHGInsurance/</url>
        <email>modules@MHGInsurance.com</email>
      </owner>
      <license src="license.txt"></license>
      <releaseNotes src="releasenotes.txt"></releaseNotes>
      <dependencies>
        <dependency type="CoreVersion">07.01.00</dependency>
      </dependencies>
      <components>
        <component type="Container">
          <containerFiles>
            <containerName>LandingSkin</containerName>
            <basepath/>
            <containerFile>
              <name>Main.ascx</name>
            </containerFile>
            <containerFile>
              <name>Blank.ascx</name>
            </containerFile>
          </containerFiles>
        </component>
      </components>
    </package>
  </packages>
</dotnetnuke>