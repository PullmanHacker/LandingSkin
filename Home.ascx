            $('#bannerImage').attr('src', 'https://www.mhgportal.com/Portals/_default/Skins/LandingSkin/img/CharterLiability_4992x3320.jpg');
            $('#txtImage').attr("src", "https://www.mhgportal.com/Portals/_default/Skins/LandingSkin/img/YCL1.jpg");
        }

        //PORTAL ID 19 = SHORE EXCURISION INSURANCE.COM 
        if (portalId == "19") {
            loadStyleSheet('https://www.mhgportal.com/Portals/_default/Skins/LandingSkin/ShoreExcursion.css');
            $('#bannerImage').attr('src', 'https://www.mhgportal.com/Portals/_default/Skins/LandingSkin/img/TOUREXCURSIONLIABILITY_3450x2574.jpg');
            $('#txtImage').attr("src", "https://www.mhgportal.com/Portals/_default/Skins/LandingSkin/img/TourExcursionInset_750x450.jpg");
        }
        //PORTAL ID 20 = INSURANCE WIKI COM 
        if (portalId == "20") {
            if (window.matchMedia('(max-width: 767px)').matches) {
            }

            $('#bannerImage').attr("src", "");
            $('#bannerImage').css("background-repeat", "no-repeat");
            $('#txtImage').attr("src", "");
        }

        //PORTAL ID 21 = YACHT CREW INSURANCE COM 
        if (portalId == "21") {
            loadStyleSheet('https://www.mhgportal.com/Portals/_default/Skins/LandingSkin/YachtCrew.css');
            $('#bannerImage').attr("src", "https://www.mhgportal.com/Portals/_default/Skins/LandingSkin/img/YachtCrewInsurance_4980x3320.jpg");
            $('#txtImage').attr("src", "https://www.mhgportal.com/Portals/_default/Skins/LandingSkin/img/YCL1.jpg");
        }
    }
</script>
