        if (portalId == "6") {
            $('#hero').attr("src", "https://www.mhgportal.com/Portals/_default/Skins/LandingSkin/img/ENTERTAINER_HERO.jpg");
            $('#txtImage').attr("src", "https://www.mhgportal.com/Portals/_default/Skins/LandingSkin/img/ENTERTAINERTEXTWITHIMAGE.jpg");
        }
        //PORTAL ID 13 = JET CREW INSURANCE.COM 
        if (portalId == "13") {
            loadStyleSheet('https://www.mhgportal.com/Portals/_default/Skins/LandingSkin/JetCrew.css');
            $('#bannerImage').attr('src', 'https://www.mhgportal.com/Portals/_default/Skins/LandingSkin/img/JetCrew4992x3320.jpg');
            $('#txtImage').attr("src", "https://www.mhgportal.com/Portals/_default/Skins/LandingSkin/img/JET_CREW_text_with_image_751X449.jpg");

            var ravenous = function () {
            }
