<input id="portalId" runat="server" type="hidden" clientidmode="Static" />
<input id="tabId" runat="server" type="hidden" clientidmode="Static" />

<script>
    $(function () {
        var portalId = <%=PortalSettings.PortalId %>;
        var tabId = <%=PortalSettings.ActiveTab.TabID %>;
        //console.log("PORTAL ID: " + portalId);
        //console.log("TAB ID: " + tabId);

        screenOrientation = ($(window).width() > $(window).height()) ? 90 : 0;
        orientationResizeFunction(screenOrientation, portalId);

    });
    function JavascriptContactUsModal_OnClick() {
        var isValid = false;
        document.getElementById('loadingPanel').style.visibility = 'visible';

        isValid = validateContactUsModalData();

        if (isValid) {
            var element = document.getElementById("contactOption");
            var selectedOption = element.options[element.selectedIndex].text;
            var postData = {
                portalId: <%=PortalSettings.PortalId %>, 
                tabId: <%=PortalSettings.ActiveTab.TabID %>,
                firstName: document.getElementById("firstName").value,
                lastName: document.getElementById("lastName").value,
                company: document.getElementById("company").value,
                phone: document.getElementById("phone").value,
                email: document.getElementById("email").value,
                comments: document.getElementById("comments").value,
                contactOption: selectedOption,
                bestTimeToContact: document.getElementById("bestTimeToContact").value
            };
            $.ajax({
                type: "POST",
                url: 'https://MHGPortal.com/DesktopModules/DNNServiceFramework/API/Lead/LandingPageGetAQuoteModal',
                data: JSON.stringify(postData),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (msg) {
                    $('#getAquote').modal('hide');
                    document.getElementById('loadingPanel').style.visibility = 'hidden';
                    $('#thankyouModal').modal('show');
                },
                error: function (msg) {
                    document.getElementById('loadingPanel').style.visibility = 'hidden';
                    alert('error' + JSON.stringify(msg));

                }
            });
        }
        else {
            document.getElementById('loadingPanel').style.visibility = 'hidden';
            return;
        }
    }
    function validateContactUsModalData() {
        var isValid = false;

        isValid = validateRequiredField(document.getElementById("firstName").value);
        addVisualIndicator(isValid, document.getElementById("firstName"));

        if (isValid) {
            isValid = validateRequiredField(document.getElementById("lastName").value);
            addVisualIndicator(isValid, document.getElementById("lastName"));
        }
        else {
            validateRequiredField(document.getElementById("lastName").value);
            addVisualIndicator(isValid, document.getElementById("lastName"));
        }
        if (isValid) {
            isValid = validateRequiredField(document.getElementById("company").value);
            addVisualIndicator(isValid, document.getElementById("company"));
        }
        else {
            validateRequiredField(document.getElementById("company").value);
            addVisualIndicator(isValid, document.getElementById("company"));
        }
        if (isValid) {
            isValid = validateRequiredField(document.getElementById("phone").value);
            addVisualIndicator(isValid, document.getElementById("phone"));
        }
        else {
            validateRequiredField(document.getElementById("phone").value);
            addVisualIndicator(isValid, document.getElementById("phone"));
        }
        if (isValid) {
            isValid = validateRequiredField(document.getElementById("email").value);
            addVisualIndicator(isValid, document.getElementById("email"));
        }
        else {
            validateRequiredField(document.getElementById("email").value);
            addVisualIndicator(isValid, document.getElementById("email"));
        }
        return isValid;
    }

    function validateOnBlur(obj) {
        var isValid = false;
        var id = obj.id;
        var value = obj.value;
        var element = document.getElementById(id);

        isValid = validateRequiredField(element.value);
        addVisualIndicator(isValid, element);
    }

    function validateRequiredField(value) {
        if (value == "" || value == null) {
            return false;
        }
        else {
            return true;
        }
    }

    function addVisualIndicator(isValid, element) {
        if (isValid) {
            element.style.borderColor = "#ccc";
        }
        else {
            element.style.borderColor = "red";
        }
    }
    function loadStyleSheet(src) {
        if (document.createStyleSheet) document.createStyleSheet(src);
        else {
            var stylesheet = document.createElement('link');
            stylesheet.href = src;
            stylesheet.rel = 'stylesheet';
            stylesheet.type = 'text/css';
            document.getElementsByTagName('head')[0].appendChild(stylesheet);
        }
    }

    $(window).bind("resize", function () {
        screenOrientation = ($(window).width() > $(window).height()) ? 90 : 0;
        //console.log("screenOrientation: " + screenOrientation);
        var portalId = <%=PortalSettings.PortalId %>;

        //console.log("PORTAL ID 2: " + portalId)
        orientationResizeFunction(screenOrientation, portalId);

    });
